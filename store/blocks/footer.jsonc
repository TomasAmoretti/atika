  "flex-layout.row#footer#newsletter":{
    "children":[
      "rich-text#Newsletter",
      "flex-layout.row#formNewsletter"
    ],
    "props":{
      "horizontalAlign":"center",
      "fullWidth": true,
      "blockClass": "footer-news"
    }
  },
  "rich-text#Newsletter": {
    "props": {
      "textPosition": "CENTER",
      "text": "**NEWSLETTER** \n\n Suscribite al newsletter y no te pierdas los lanzamientos de Atika!",
      "blockClass": "footer-newsletter-text"
    }
  },
  "flex-layout.row#formNewsletter": {
    "children": [
      "flex-layout.col#formNewsletter"
    ],
    "props":{
      "blockClass": "row-newsletter"
    }
  },
  "flex-layout.col#formNewsletter": {
    "children": [
      "newsletter"
    ],
    "props":{
      "blockClass": "form-newsletter"
    }
  },
  "newsletter":{
    "props":{
      "hideLabel": true,
      "submit": "Enviar",
      "placeholder":"Email",
      "blockClass": "newsletter"
    }
  },
  "flex-layout.row#footer-bottom": { 
    "children": [
      "image#vtex",
      "image#ko-logo"
    ],
    "props":{
      "blockClass":"footer-bottom",
      "verticalAlign": "center",
      "preventVerticalStretch": true
    }
  },
  "image#vtex": {
    "props": {
      "width": 100,
      "src": "https://brand.vtex.com/static/media/VTEX_pink_RGB.751a9fb5.svg",
      "url":{
        "link":"https://vtex.com/"
      },
      "blockClass": "logo-vtex"
    }
  },
  "image#ko-logo": {
    "props": {
      "src": "/arquivos/logo_ko.png",
      "url":{
        "link":"https://knownonline.com/"
      },
      "width": 150,
      "height": 30,
      "blockClass": "logo-ko"
    }
  },
  "footer-layout.mobile": { 
    "children": [
      "flex-layout.row#footer#newsletter",
      "flex-layout.row#footer-bottom"
    ]
  }
