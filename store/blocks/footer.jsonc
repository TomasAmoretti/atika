      "image#logo-footer",
      "flex-layout.col#footer-general-item-1",
      "flex-layout.col#footer-general-item-2",
      "flex-layout.col#footer-general-item-3",
      "flex-layout.col#footer-general-item-4"
    ],
    "props":{
      "blockClass": "footer-general-fila-1"
    }
  },
  "image#logo-footer":{
    "props": {
      "src": "https://dummyimage.com/150x40/000/fff",
      "link":{
        "url":"/"
      },
      "blockClass": "footer-logo"
    }
