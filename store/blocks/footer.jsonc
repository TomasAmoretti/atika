{
  "footer": {
    "blocks": ["footer-layout.desktop", "footer-layout.mobile"]
  },
  "footer-layout.desktop": { 
    "children": [
      "flex-layout.row#footer#newsletter",
      "flex-layout.row#footer-general",
      "flex-layout.row#footer-bottom"
    ]
  },
  "flex-layout.row#footer-general":{
    "children":[
      "flex-layout.col#footer-general"
    ],
    "props":{
      "blockClass": "footer-general"
    }
  },
  "flex-layout.col#footer-general":{
    "children":[
      "flex-layout.row#footer-general-fila-1",
      "flex-layout.row#footer-general-fila-2"
    ],
    "props":{
      "blockClass": "col-footer-general"
    }
