{
    "store.custom#marcas-instuticional":{
        "blocks": [
            "responsive-layout.desktop#desktop-marcas-instuticional",
            "responsive-layout.mobile#mobile-marcas-instuticional"
        ]
    },
    "responsive-layout.desktop#desktop-marcas-instuticional": {
        "children": [
            "image#banner-marcas-instuticional",
            "flex-layout.row#marcas-contenedor"
        ]
    },
    "image#banner-marcas-instuticional":{
        "props":{
            "src": "/arquivos/Pagina Estatica- Marcas_header.png",
            "width": "100%",
            "blockClass":"banner-marcas-instuticional"
        }
    },
    "flex-layout.row#marcas-contenedor":{
        "children":[
            "flex-layout.col#marcas-contenedor"
        ],
        "props":{
            "blockClass":"contenedor-marcas"
        }
    },
    "flex-layout.col#marcas-contenedor":{
        "children":[
            "flex-layout.row#marcas-item-1"
        ],
        "props":{
            "blockClass": "col-contenedor-marcas",
            "preventVerticalStretch": true
        }
    },
    "flex-layout.row#marcas-item-1":{
        "children":[
            "image#marca-item-1",
            "flex-layout.col#marcas-item-1"
        ],
        "props":{
            "blockClass":"item-marcas",
            "preventHorizontalStretch":true
        }
    },
    "image#marca-item-1":{
        "props":{
            "src": "https://dummyimage.com/150x70/000/fff",
            "blockClass":"marca-institucional"
        }
    },
    "flex-layout.col#marcas-item-1":{
        "children":[
            "flex-layout.row#marcas-texto-item-1",
            "flex-layout.row#marcas-texto-item-2",
            "flex-layout.row#marcas-texto-item-3",
            "flex-layout.row#marcas-texto-item-4",
            "flex-layout.row#marcas-texto-item-5",
            "flex-layout.row#marcas-texto-item-6",
            "flex-layout.row#marcas-texto-item-7"
        ],
        "props":{
            "blockClass":"col-item-marcas",
            "preventVerticalStretch":true
        }
    },
    "flex-layout.row#marcas-texto-item-1":{
        "children":[
            "rich-text#marcas-item-1-left",
            "rich-text#marcas-item-1-right"
        ],
        "props":{
